<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"     
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kk.dao.OrderDao">
	<insert id="saveOrder" parameterType="order">
		INSERT INTO marketCart(userName,userCookId,orderPrice,orderUserRealName,orderUserAddress,orderUserMobil,orderWeight,orderReTime)
		VALUES(#{userName},#{userCookId},#{orderPrice},#{orderUserRealName},#{marketProductNum},#{orderUserAddress},#{orderUserMobil},#{orderWeight},#{orderReTime})
	</insert>
</mapper>